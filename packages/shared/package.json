{
  "name": "@dwidget/shared",
  "version": "1.0.0",
  "description": "Shared codebase for dwidgets and dapps",
  "scripts": {
    "build:proto": "pnpm dlx protoc -I proto --ts_opt generate_dependencies --ts_out proto --ts_opt optimize_code_size etherean.proto assets.proto history.proto"
  },
  "dependencies": {
    "@protobuf-ts/grpcweb-transport": "^2.9.1",
    "@protobuf-ts/runtime": "^2.9.1",
    "clsx": "^2.0.0",
    "decimal.js": "^10.4.3",
    "material-symbols": "^0.13.2",
    "tailwind-material-colors": "^1.1.3",
    "tailwind-merge": "^1.14.0",
    "toformat": "^2.0.0",
    "windstitch": "^0.3.0"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.9.1"
  },
  "peerDependencies": {
    "@protobuf-ts/runtime-rpc": "^2.9.1",
    "@tanstack/react-query": "^5.0.0",
    "preact": "^10.16.0",
    "tailwindcss": "^3.0.0",
    "viem": "^1.16.6",
    "wagmi": "^1.4.5"
  },
  "pnpm": {
    "overrides": {
      "tailwindcss": "^3.3.5"
    },
    "packageExtensions": {
      "tailwind-material-colors": {
        "peerDependencies": {
          "tailwindcss": "^3.3.5"
        }
      }
    }
  }
}